# from tvm.script import ir as I
# from tvm.script import tir as T

@I.ir_module
class Module:
    @T.prim_func
    def main(data: T.Buffer((1, 15, 13, 7), "float32"), adaptive_pool_avg: T.Buffer((1, 15, 1, 1), "float32")):
        T.func_attr({"from_legacy_te_schedule": T.bool(True), "tir.noalias": T.bool(True)})
        adaptive_pool_sum = T.allocate([1], "float32", "global")
        for ax1 in range(15):
            adaptive_pool_sum_1 = T.Buffer((1,), data=adaptive_pool_sum, align=4)
            adaptive_pool_sum_1[0] = T.float32(0)
            for rv0, rv1 in T.grid(13, 7):
                data_1 = T.Buffer((1365,), data=data.data)
                adaptive_pool_sum_1[0] = adaptive_pool_sum_1[0] + data_1[ax1 * 91 + rv0 * 7 + rv1]
            adaptive_pool_avg_1 = T.Buffer((15,), data=adaptive_pool_avg.data)
            adaptive_pool_avg_1[ax1] = adaptive_pool_sum_1[0] * T.float32(0.01098901098901099)