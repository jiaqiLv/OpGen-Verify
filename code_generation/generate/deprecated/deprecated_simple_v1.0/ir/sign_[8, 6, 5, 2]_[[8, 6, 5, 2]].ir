# from tvm.script import ir as I
# from tvm.script import tir as T

@I.ir_module
class Module:
    @T.prim_func
    def main(data: T.Buffer((8, 6, 5, 2), "float32"), T_sign: T.Buffer((8, 6, 5, 2), "float32")):
        T.func_attr({"from_legacy_te_schedule": T.bool(True), "tir.noalias": T.bool(True)})
        for ax0 in T.parallel(8):
            for ax1, ax2, ax3 in T.grid(6, 5, 2):
                cse_var_1: T.int32 = ax0 * 60 + ax1 * 10 + ax2 * 2 + ax3
                T_sign_1 = T.Buffer((480,), data=T_sign.data)
                data_1 = T.Buffer((480,), data=data.data)
                T_sign_1[cse_var_1] = T.if_then_else(T.float32(0) < data_1[cse_var_1], T.float32(1), T.Select(data_1[cse_var_1] < T.float32(0), T.float32(-1), T.float32(0)))