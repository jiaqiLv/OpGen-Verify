@I.ir_module
class Module:
    @T.prim_func
    def main(data: T.Buffer((6, 8, 1, 3), "float32"), T_sign: T.Buffer((6, 8, 1, 3), "float32")):
        T.func_attr({"from_legacy_te_schedule": T.bool(True), "tir.noalias": T.bool(True)})
        for ax0_ax1_fused in T.parallel(48):
            for ax3 in range(3):
                cse_var_1: T.int32 = ax0_ax1_fused * 3 + ax3
                T_sign_1 = T.Buffer((144,), data=T_sign.data)
                data_1 = T.Buffer((144,), data=data.data)
                T_sign_1[cse_var_1] = T.if_then_else(T.float32(0) < data_1[cse_var_1], T.float32(1), T.Select(data_1[cse_var_1] < T.float32(0), T.float32(-1), T.float32(0)))