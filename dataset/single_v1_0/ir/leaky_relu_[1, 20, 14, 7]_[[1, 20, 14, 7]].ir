# from tvm.script import ir as I
# from tvm.script import tir as T

@I.ir_module
class Module:
    @T.prim_func
    def main(data: T.Buffer((1, 20, 14, 7), "float32"), compute: T.Buffer((1, 20, 14, 7), "float32")):
        T.func_attr({"from_legacy_te_schedule": T.bool(True), "tir.noalias": T.bool(True)})
        for i1, i2, i3 in T.grid(20, 14, 7):
            cse_var_1: T.int32 = i1 * 98 + i2 * 7 + i3
            compute_1 = T.Buffer((1960,), data=compute.data)
            data_1 = T.Buffer((1960,), data=data.data)
            compute_1[cse_var_1] = T.if_then_else(T.float32(0) < data_1[cse_var_1], data_1[cse_var_1], data_1[cse_var_1] * T.float32(0.5))