# from tvm.script import ir as I
# from tvm.script import tir as T

@I.ir_module
class Module:
    @T.prim_func
    def main(data: T.Buffer((10, 3, 8, 9), "float32"), compute: T.Buffer((10, 3, 8, 9), "float32")):
        T.func_attr({"from_legacy_te_schedule": T.bool(True), "tir.noalias": T.bool(True)})
        for i0 in T.parallel(10):
            for i1, i2, i3 in T.grid(3, 8, 9):
                cse_var_1: T.int32 = i0 * 216 + i1 * 72 + i2 * 9 + i3
                compute_1 = T.Buffer((2160,), data=compute.data)
                data_1 = T.Buffer((2160,), data=data.data)
                compute_1[cse_var_1] = T.erf(data_1[cse_var_1])